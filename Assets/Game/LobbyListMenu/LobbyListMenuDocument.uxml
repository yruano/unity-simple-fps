<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/Game/LobbyDashboardMenu/LobbyDashboardMenu.uss?fileID=7433441132597879392&amp;guid=33b6bbb7657bbba4cb2664958db46c3f&amp;type=3#LobbyDashboardMenu" />
    <engine:VisualElement style="padding-top: 20px; padding-right: 20px; padding-bottom: 20px; padding-left: 20px; flex-direction: row; flex-grow: 1;">
        <engine:VisualElement style="flex-grow: 0; background-color: rgb(43, 43, 43); margin-right: 5px; width: 400px; flex-shrink: 0;">
            <engine:Button text="Refresh" name="RefreshButton" style="margin-top: 4px; margin-right: 4px; margin-bottom: 4px; margin-left: 4px; padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px;" />
            <engine:ListView name="LobbyListView" virtualization-method="DynamicHeight" style="flex-grow: 1; margin-top: 4px; margin-right: 4px; margin-bottom: 4px; margin-left: 4px;" />
        </engine:VisualElement>
        <engine:VisualElement style="flex-grow: 1; margin-left: 5px; background-color: rgb(43, 43, 43);">
            <engine:VisualElement style="flex-direction: row; align-items: center;">
                <engine:TextField placeholder-text="Lobby name" name="LobbyNameTextField" style="margin-top: 4px; width: 400px;" />
                <engine:Label text="Max players:" style="color: rgb(255, 255, 255); margin-top: 2px; margin-right: 2px; margin-bottom: 2px; margin-left: 4px; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0;" />
                <engine:IntegerField value="10" max-length="2" name="MaxPlayersNum" style="margin-top: 2px; margin-right: 2px; margin-bottom: 2px; margin-left: 2px;" />
                <engine:Label text="Friend only:" style="color: rgb(255, 255, 255); margin-top: 2px; margin-right: 2px; margin-bottom: 2px; margin-left: 4px; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0;" />
                <engine:Toggle name="FriendOnlyToggle" style="margin-top: 2px; margin-right: 2px; margin-bottom: 2px; margin-left: 2px;" />
                <engine:Button text="Create lobby" name="LobbyButton1" style="margin-left: auto; margin-right: 2px; padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px;" />
                <engine:Button text="Delete lobby" name="LobbyButton2" style="padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px; margin-top: 2px; margin-right: 4px; margin-bottom: 2px; margin-left: 2px;" />
            </engine:VisualElement>
            <engine:VisualElement style="flex-grow: 1; flex-direction: row;">
                <engine:VisualElement style="background-color: rgb(34, 34, 34); margin-top: 4px; margin-right: 4px; margin-bottom: 4px; margin-left: 4px; width: 400px;">
                    <engine:ListView name="LobbyMemberListView" virtualization-method="DynamicHeight" />
                </engine:VisualElement>
                <engine:VisualElement style="flex-grow: 1;">
                    <engine:VisualElement style="flex-direction: row; flex-shrink: 1; flex-grow: 1;">
                        <engine:VisualElement style="flex-grow: 0; padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px; width: 400px;">
                            <engine:VisualElement style="flex-grow: 1;" />
                        </engine:VisualElement>
                        <engine:VisualElement style="flex-grow: 1;">
                            <engine:ListView />
                        </engine:VisualElement>
                    </engine:VisualElement>
                    <engine:VisualElement style="flex-grow: 0; flex-shrink: 0; flex-direction: row; height: 400px;">
                        <engine:VisualElement style="flex-grow: 1;">
                            <engine:ListView name="LobbyChatHistoryListView" virtualization-method="DynamicHeight" selection-type="None" class="chatHistoryListView" style="flex-grow: 1;" />
                            <engine:TextField placeholder-text="Chat message..." name="LobbyChatTextField" is-delayed="true" style="margin-top: 2px; margin-right: 2px; margin-bottom: 4px; margin-left: 2px;" />
                        </engine:VisualElement>
                        <engine:VisualElement style="justify-content: flex-end;">
                            <engine:Button text="Start match" name="StartMatchButton" style="margin-top: 2px; margin-right: 4px; margin-bottom: 4px; margin-left: 2px; padding-top: 10px; padding-right: 10px; padding-bottom: 10px; padding-left: 10px;" />
                        </engine:VisualElement>
                    </engine:VisualElement>
                </engine:VisualElement>
            </engine:VisualElement>
        </engine:VisualElement>
    </engine:VisualElement>
</engine:UXML>
